r\n        res.send(\"unable to remove the invoice\");\r\n      } else {\r\n        console.log(Invoice);\r\n        res.status(204);\r\n      }\r\n    }\r\n  );\r\n});\r\nmodule.exports = router;\r\n"},"lineCount":null}},"error":null,"hash":"4918e2b215fe6bd5894b2adeffdd3c4b","cacheData":{"env":{}}}